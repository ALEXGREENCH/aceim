<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aceim.protocol.snuk182.xmpp"
    android:installLocation="internalOnly"
    android:versionCode="3"
    android:versionName="0.9.3" >

    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="aceim.api.application.AceIMBaseApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <service
            android:name="aceim.protocol.snuk182.xmpp.XMPPProtocol"
            android:exported="true"
            android:process="aceim.protocol.snuk182.xmpp" >
            <intent-filter>
                <action android:name="aceim.protocol" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>
    </application>

</manifest>