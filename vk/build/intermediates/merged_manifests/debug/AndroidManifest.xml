<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aceim.protocol.snuk182.vkontakte"
    android:installLocation="internalOnly"
    android:versionCode="3"
    android:versionName="0.9.3.1" >

    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
/>
    <application
        android:name="aceim.api.application.AceIMBaseApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <service
            android:name="aceim.protocol.snuk182.vkontakte.VkProtocol"
            android:exported="true"
            android:process="aceim.protocol.snuk182.vkontakte" >
            <intent-filter>
                <action android:name="aceim.protocol" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>

        <activity
            android:name="aceim.protocol.snuk182.vkontakte.internal.LoginActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="standard"
            android:theme="@android:style/Theme.Dialog" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>